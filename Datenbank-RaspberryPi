CREATE TABLE `studyzone`.`Benutzer` (`ID` INT NOT NULL AUTO_INCREMENT , `Identit√§t` VARCHAR(50) NOT NULL , `Name` VARCHAR(50) NOT NULL , `E-Mail` VARCHAR(50) NOT NULL , `Passwort` VARCHAR(50) NOT NULL , PRIMARY KEY (`ID`)) ENGINE = InnoDB;
CREATE TABLE `studyzone`.`Fragen` (`ID` INT NOT NULL AUTO_INCREMENT , `Fragetyp` VARCHAR(100) NOT NULL , `Fragetext` VARCHAR(500) NOT NULL , `Fach_ID` INT NOT NULL , `Thema_ID` INT NOT NULL , PRIMARY KEY (`ID`)) ENGINE = InnoDB;
